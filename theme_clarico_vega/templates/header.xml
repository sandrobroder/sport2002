                        <div class="te_header_search">
                            <div class="te_search_popover" tabindex="-1">
                                <t t-call="theme_clarico_vega.website_search_box_input_ept"/>
                            </div>
                        </div>
                    </div>
                    <div class="te_header_right_icon">
                        <div class="te_header_search">
                            <div class="te_srch_icon_header">
                                <a href="#" class="">
                                    <i class="fa fa-search te_srch_icon"/>
                                    <i class="fa fa-times te_srch_close"/>
                                </a>
                            </div>
                        </div>
                        <t t-call="theme_clarico_vega.header_components"/>
                        <t t-if="request.website.viewref('theme_clarico_vega.place_in_header').active">
                            <t t-call="theme_clarico_vega.lang_pricelist_block"/>
                        </t>
                    </div>
                </div>
            </nav>
        </xpath>
    </template>

    <template id="option_header_demo" inherit_id="website.navbar_nav" name="header">
        <xpath expr="//ul[@id='top_menu']" position="inside">
            <span id="hide_wishlist">
            <t t-nocache="The wishlist depends on the user and must not be shared with other users. The product come from the controller." t-call="website_sale_wishlist.header_wishlist_link"
                t-nocache-_icon="_icon"
                t-nocache-_item_class="_item_class"
                t-nocache-_link_class="_link_class">
                <t t-set="_icon" t-value="True"/>
                <t t-set="_item_class" t-value="'nav-item'"/>
                <t t-set="_link_class" t-value="'nav-link'"/>
            </t></span>
        </xpath>
    </template>

    <!-- Search for odoo header styles -->
    <template id="te_search_bar" inherit_id="website_sale.header_cart_link">
        <xpath expr="//li[contains(@t-attf-class, 'o_wsale_my_cart')]" position="before">
            <li t-if="not search_icon_disabled" t-attf-class="nav-item #{_item_class} te_srch_icon_header_default">
                <t t-if="request.website.viewref('website.template_header_default').active">
                    <t t-set="_item_class" t-value="'mx-lg-3'"/>
                </t>
                <t t-if="request.website.viewref('website.template_header_vertical').active or
                request.website.viewref('website.template_header_contact').active or
                request.website.viewref('website.template_header_image').active or
                request.website.viewref('website.template_header_hamburger_full').active">
                    <t t-set="_item_class" t-value="'ml-lg-3'"/>
                </t>
                <t t-if="request.website.viewref('website.template_header_magazine').active">
                    <t t-set="_item_class" t-value="'ml-auto'"/>
                </t>
                <a href="#" class="nav-link">
                    <i class="fa fa-search te_srch_icon"/>
                </a>
                <div class="te_searchform__popup" id="searchForm">
                    <a href="#" class="te_btn-close">
                        <i class="fa fa-times te_srch_close"/>
                    </a>
                    <div class="te_searchform__body">
                        <div class="te_text">Start typing and press Enter to search</div>
                        <t t-call="theme_clarico_vega.website_search_box_input_ept"/>
                    </div>
                </div>
            </li>
        </xpath>
    </template>
</odoo>