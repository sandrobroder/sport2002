                                    </li>
                                </ul>
                            </t>
                        </div>
                    </div>
                </div>
            </nav>
        </xpath>
    </template>

    <!-- ======================= Header style 3 ========================= -->
    <template id="template_header_style_3" inherit_id="website.layout"
              name="Vega style 3" active="False">
        <xpath expr="//header//nav" position="replace">
            <t t-call="theme_clarico_vega.te_header_usermenu"/>
